{
  "name": "slack-aqi",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "functions/slack-aqi.js",
  "scripts": {
    "start": "NODE_ENV=dev npm run start:server",
    "start:server": "npx netlify-lambda serve functions",
    "build": "npm run build:functions",
    "build:functions": "npx netlify-lambda build functions"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@slack/client": "^4.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://github.com/pamo/slack-update-aqi-topic"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "netlify"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "netlify-lambda": "^1.0.3",
    "npm-run-all": "^4.1.3"
  }
}
